{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "source": "./public/js/index.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "debug": "ndb server.js",
    "build": "parcel build ./public/js/index.js --out-dir ./public/js/bundled --public-url /js/bundled",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --out-dir ./public/js/bundled --public-url /js/bundled"
  },
  "author": "ThHao",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-csp": "^0.1.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "sharp": "^0.33.0",
    "slugify": "^1.6.6",
    "stripe": "^14.7.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
